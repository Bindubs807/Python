import matplotlib.pyplot as plt
import numpy as np
no=[39,49, 43, 45, 14, 16, 58, 41, 15, 18, 44, 19, 21, 48, 38, 32, 11, 57, 48, 54 ,45, 
30, 43, 35, 48, 36, 48, 44, 10, 49, 27, 48, 21, 18 ,17 ,13 ,47 ,27, 10, 35, 59, 19, 35, 
32, 32, 23, 32, 15, 42 ,43]
plt.hist(no, color="b" , edgecolor="r", label="Histogram") 
plt.title("Histogram Program")
plt.xlabel("Histogram") 
plt.ylabel("Number")
plt.axvline(35, color="g", label="line")
plt.legend( ) 
plt.grid( ) 
plt.show( )
